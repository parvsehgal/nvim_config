{
  "languageserver": {
    "prismals": {
      "command": "prisma-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "prisma"
      ],
      "rootPatterns": [
        "prisma/schema.prisma"
      ],
      "initializationOptions": {
        "prismaLocation": ""
      }
    }
  }
}
